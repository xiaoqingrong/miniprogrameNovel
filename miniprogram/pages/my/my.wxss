#myBook{
  font-size: 30rpx;
  padding: 0 10px;
  text-align: center;
}
#myBook view image{
  width: 50rpx;
  margin: 0 8rpx;
  vertical-align: middle;
}
#myBook>image{
  width: 40%;
  border-radius: 50%;
}
#myBook view image:last-child{
  float: right;
  width: 40rpx;
  margin-top: 10rpx;
}
#myBook view{
  height: 70rpx;
  line-height: 70rpx;
  text-align: left;
  border: 1px solid #ccc;
  margin: 10px 0;
}
